<header>
  <div id="summary_title">
    <h1>Listing users</h1>
  </div>
</header>
<section>
  <article>
    <table>
      <tr>
        <th>Tweet</th>
        <th>Screen name</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
      <% @users.each do |user| %>
      <tr>
        <td><%= user.tweet_id %></td>
        <td><%= user.screen_name %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
      </tr>
      <% end %>
    </table>
  </article>
</section>
